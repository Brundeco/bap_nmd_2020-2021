)\n      .then((res) => {\n        let arr = messages\n        arr.push(res.data)\n        setMessages([...arr])\n        socket.emit('new-message', {\n          id: match.params.recepient_id,\n          message: res.data.message,\n        })\n        setData((prev) => ({\n          ...prev,\n          message: '',\n        }))\n      })\n      .catch((err) => console.log(err))\n  }\n\n  return (\n    <div className=\"dashboard-screen \">\n      {/* <Header /> */}\n      <div className=\"chat-screen page-wrapper\">\n        <div className=\"chat-top\">\n          <h1> Gesprek met {match.params.recepient}</h1>\n        </div>\n\n        <ScrollToBottom debsug={false}>\n          <section className=\"conversation-box\">\n            {messages?.map((message, i) => {\n              return (\n                <p\n                  key={i}\n                  className={\n                    message.fromName == user.username\n                      ? 'float-right'\n                      : 'float-left'\n                  }\n                >\n                  {message.message}\n                </p>\n              )\n            })}\n          </section>\n        </ScrollToBottom>\n\n        <div className=\"chat-bottom\">\n          <textarea\n            className=\"send-message\"\n            placeholder=\"message\"\n            name=\"message\"\n            onChange={(e) => handleChange('message', e.target.value)}\n            value={data.message}\n            required=\"required\"\n          />\n          <button className=\"send-message-btn\" onClick={() => postMessage()}>\n            <img src={PaperplaneIcon} alt=\"\" />\n          </button>\n        </div>\n      </div>\n    </div>\n  )\n}\n","/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/pages/Users/CreateUser.js",["342","343"],"import React, { useState, useEffect } from 'react'\nimport { InputField, Preloader } from './../../components'\nimport FileBase from 'react-file-base64'\nimport axios fro