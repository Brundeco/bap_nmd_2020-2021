t handleSubmit = (e) => {\n    e.preventDefault()\n    setProgress(true)\n    if (data.password !== data.passwordRepeat) {\n      setStatus('Passwords do not match')\n    } else {\n      axios\n        .post('http://localhost:5000/users/register', data)\n        .then((res) => {\n          setProgress(false)\n          setStatus(res.data.message)\n          window.location = '/login?email=' + res.data.user.email\n        })\n        .catch((err) => {\n          setProgress(false)\n          setStatus(err.response.data.message)\n        })\n    }\n  }\n\n  return (\n    <div className=\"login-screen padding-correction\">\n      <div className={progress ? 'await-result show' : 'await-result hide'}>\n        {progress ? <PreloaderSpinningWheel text=\"Registering account\" /> : ''}\n      </div>\n      <div className=\"wrapper\">\n        {/* <img src={TextLogo} alt=\"Suitswap logo\" className=\"logo\" /> */}\n        <h1>Create account</h1>\n        <form action=\"\" onSubmit={handleSubmit}>\n          <div></div>\n          <InputField\n            name=\"username\"\n            placeholder=\"Username\"\n            type=\"text\"\n            onChange={handleChange}\n            required\n          />\n          <InputField\n            name=\"phone\"\n            placeholder=\"Phone\"\n            type=\"text\"\n            onChange={handleChange}\n            required\n          />\n          <InputField\n            name=\"email\"\n            placeholder=\"Email\"\n            type=\"email\"\n            onChange={handleChange}\n            required\n          />\n          <InputField\n            name=\"password\"\n            placeholder=\"Password\"\n            type=\"password\"\n            onChange={handleChange}\n            required\n          />\n          <InputField\n            name=\"passwordRepeat\"\n            placeholder=\"Repeat password\"\n            type=\"password\"\n            onChange={handleChange}\n            required\n          />\n  