an)\n    const promisesArr = promises?.flat()\n    promisesArr &&\n      Promise.all(promisesArr).then((newArray) => {\n        setImages((prevImgs) => [...prevImgs, ...newArray])\n      })\n  }, [property])\n\n  return (\n    <div className=\"reservation-detail-screen\">\n      <button className=\"close-btn\" onClick={() => history.goBack()}>\n        <img src={CloseIcon} alt=\"close button\" />\n      </button>\n\n      <section>\n        <h2 className=\"main-title\">Details about the reservation</h2>\n\n        <div className=\"info-group\">\n          <p className=\"semi-bold\">Amount paid</p>\n          <p>€{reservation?.price}</p>\n        </div>\n        <div className=\"info-group\">\n          <p className=\"semi-bold\">Payment date</p>\n          <p>{ConvertDate(reservation?.createdAt)}</p>\n        </div>\n\n        <div className=\"info-group dates flex-col\">\n          <p className=\"semi-bold\">Reserved dates</p>\n          <DayPicker selectedDays={dates} disabledDays={isDayDisabled} />\n        </div>\n      </section>\n\n      <section>\n        <h2 className=\"main-title\">Details about the property</h2>\n        <div className=\"info-group\">\n          <p className=\"semi-bold\">Dailyprice</p>\n          <p>€{property?.price}</p>\n        </div>\n        <div className=\"info-group\">\n          <p className=\"semi-bold\">Space</p>\n          <p>{property?.surface} m2</p>\n        </div>\n        <div className=\"info-group\">\n          <p className=\"semi-bold\">Ares</p>\n          <p>{property?.areas}</p>\n        </div>\n        <div className=\"info-group\">\n          <p className=\"semi-bold\">Address</p>\n          <p>{`${property?.street} ${property?.houseNumber}, ${property?.zip} ${property?.city}`}</p>\n        </div>\n      </section>\n\n      <section>\n        <Swiper slides={images} />\n      </section>\n\n      <section>\n        <h2 className=\"main-title\">\n          For further questions about your reservation we would like you to\n          contact the property owner. Please use the contact details below.{' '}\n        </h2>\n        <section className=\"cta-bottom-section\">\n          <button className={'main-btn'} onClick={() => {}}>\n            <img src={PhoneIcon} alt=\"\" /> {`${property?.phone}`}\n          </button>\n          <button\n            onClick={() =>\n              (window.location = `/chat/${property?.author_id}/${property?.author}/${conversationId}`)\n            }\n          >\n            Chat with owner\n          </button>\n        </section>\n      </section>\n    </div>\n  )\n}\n","/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/pages/Dashboard/Profile/Profile.js",["529","530","531"],"/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/functions/DeleteUserContent.js",[],{"ruleId":"532","replacedBy":"533"},{"ruleId":"534","replacedBy":"535"},{"ruleId":"536","severity":1,"message":"537","line":1,"column":17,"nodeType":"538","messageId":"539","endLine":1,"endColumn":25},{"ruleId":"536","severity":1,"message":"540","line":1,"column":27,"nodeType":"538","messageId":"539","endLine":1,"endColumn":36},{"ruleId":"541","severity":1,"message":"542","line":4,"column":1,"nodeType":"543","endLine":12,"endColumn":3},{"ruleId":"536","severity":1,"message":"540","line":1,"column":17,"nodeType":"538","messageId":"539","endLine":1,"endColumn":26},{"ruleId":"536","severity":1,"message":"537","line":1,"column":28,"nodeType":"538","messageId":"539","endLine":1,"endColumn":36},{"ruleId":"541","severity":1,"message":"542","line":3,"column":1,"nodeType":"543","endLine":22,"endColumn":3},{"ruleId":"536","severity":1,"message":"544","line":4,"column":10,"nodeType":"538","messageId":"539","endLine":4,"endColumn":15},{"ruleId":"541","severity":1,"message":"542","line":3,"column":1,"nodeType":"543","endLine":24,"endColumn":2},{"ruleId":"541","severity":1,"message":"542","line":3,"column":1,"nodeType":"543","endLine":22,"endColumn":2},{"ruleId":"536","sever