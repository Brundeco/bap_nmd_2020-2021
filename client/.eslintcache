a != undefined) {\n    return (\n      <div>\n        <div className=\"event-screen\">\n          <div className=\"subject-image\">\n            <img src={image} alt=\"\" />\n          </div>\n          <div className=\"wrapper\">\n            <PrevPage />\n            <h1>{data?.title}</h1>\n            <h2>By Leda Lenskens | {timestamp} </h2>\n            <p> {data?.description}</p>\n            <div>\n              <h3>Practical</h3>\n            </div>\n            <section className=\"cta-section\">\n              <button\n                className={\n                  liked ? 'main-btn liked-event' : 'main-btn not-liked-event'\n                }\n                onClick={(e) => handleClick(e)}\n              >\n                {liked ? 'Like' : 'Unlike'}\n              </button>\n            </section>\n          </div>\n        </div>\n      </div>\n    )\n  } else {\n    return (\n      <React.Fragment>\n        <Preloader />\n      </React.Fragment>\n    )\n  }\n}\n","/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/pages/Properties/CreateProperty.js",["240","241"],"/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/pages/Events/GetEventList.js",["242","243","244","245","246","247","248","249","250","251","252","253"],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport axios from 'axios'\nimport { Preloader, CheckSession, Map, ConvertDate } from './../../components'\nimport { app } from '../../base'\nimport Geocode from 'react-geocode'\nimport { getPreciseDistance } from 'geolib'\nimport likeIcon from './../../icons/heart-full-blue.svg'\n\nexport default (props) => {\n  CheckSession(localStorage.getItem('jwt'))\n\n  Geocode.setApiKey(process.env.REACT_APP_GEOCODING_API_KEY)\n  Geocode.enableDebug()\n\n  const [data, setData] = useState()\n  const storageRef = app.storage().ref()\n  const [images, setImages] = useState([])\n  const [evtsFiltered, setEvtsFiltered] = useState([])\