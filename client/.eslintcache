lToBottom()\n  }, [messages])\n\n  const onKeyPress = async (e) => {\n    if (e.which === 13) {\n      postMessage()\n    }\n  }\n\n  return (\n    <React.Fragment>\n      <Header\n        locationsharing={() => {}}\n        radius={() => {}}\n        showfilters={() => {}}\n      />\n      <div className=\"chat-screen\">\n        <section>\n          <div className=\"chat-info\">\n            <h1 className=\"main-title\">\n              Conversation with {match.params.recepient}\n            </h1>\n          </div>\n        </section>\n\n        <section>\n          <div className=\"chat-content\">\n            <section className=\"conversation-wrapper\">\n              {messages?.map((message, i) => {\n                return (\n                  <div\n                    key={i}\n                    className={\n                      message.fromName == user.username\n                        ? 'float-right'\n                        : 'float-left'\n                    }\n                  >\n                    <p>{message.message}</p>\n                  </div>\n                )\n              })}\n              <div ref={messagesEndRef} />\n            </section>\n          </div>\n        </section>\n\n        <div className=\"chat-cta\">\n          <textarea\n            className=\"send-message\"\n            placeholder=\"message\"\n            name=\"message\"\n            onChange={(e) => handleChange('message', e.target.value)}\n            value={data.message}\n            required=\"required\"\n            onKeyPress={onKeyPress}\n          />\n          <button className=\"send-message-btn\" onClick={() => postMessage()}>\n            <img src={PaperplaneIcon} alt=\"\" />\n            <p></p>\n          </button>\n        </div>\n      </div>\n    </React.Fragment>\n  )\n}\n","/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/pages/Users/CreateUser.js",["360","361"],"/Users/brunodecoene/Documents/github/bap_nmd_2020-2021/client/src/pages